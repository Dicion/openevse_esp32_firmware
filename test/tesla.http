Content-Type: application/json

{
  "tesla_enabled": true,
  "tesla_access_token": "{{tesla_access_token}}",
  "tesla_refresh_token": "{{tesla_refresh_token}}",
  "tesla_created_at": "{{tesla_created_at}}",
  "tesla_expires_in": "{{tesla_expires_in}}"
}

